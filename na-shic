control_symbol = input()
n = int(input())
max_count = -1
max_string = ""
for i in range(n):
    string = input()
    count = 0
    unique_chars = set()
    for char in string:
        if char != control_symbol and char not in unique_chars:
            count += 1
            unique_chars.add(char)
    if count > max_count:
        max_count = count
        max_string = string
unique_chars = set()
for char in max_string:
    if char != control_symbol and char not in unique_chars:
        unique_chars.add(char)
        print(char, end='')
